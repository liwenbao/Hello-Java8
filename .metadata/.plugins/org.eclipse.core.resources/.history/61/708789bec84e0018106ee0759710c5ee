public class SingletonPattern {

}

class Singleton1{
	
	private static Singleton1 instance = new Singleton1();
	
	private Singleton1() {}
	
	public Singleton1 getInstance() {
		return instance;
	}
}

class Singleton2{
	
	private static Singleton1 instance;
	
	private Singleton1() {}
	
	public Singleton1 getInstance() {
		return instance;
	}
}
