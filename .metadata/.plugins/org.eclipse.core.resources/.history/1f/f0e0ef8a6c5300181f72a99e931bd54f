package function;

import java.util.function.Function;

public class FunctionTest {
	public static void main (String[] args) {
		Function<Integer, Double> f1 = t -> Math.sqrt(t);
		System.out.println("f1.apply(1) = " + f1.apply(1));
		System.out.println("f1.apply(2) = " + f1.apply(2));
		System.out.println("f1.apply(4) = " + f1.apply(4));
		
	}
}
